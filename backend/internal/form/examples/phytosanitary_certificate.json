            },
            {
                "type": "Control",
                "scope": "#/properties/originCountry"
            },
            {
                "type": "Control",
                "scope": "#/properties/exportCountry"
            },
            {
                "type": "Control",
                "scope": "#/properties/packageDescription"
            },
            {
                "type": "Control",
                "scope": "#/properties/numberOfPackages"
            },
            {
                "type": "Control",
                "scope": "#/properties/quantityDeclared"
